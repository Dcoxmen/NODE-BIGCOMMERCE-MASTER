
    <div class="row">
      
          <div class="col-12">
            <div><h2>{{product_name}}</h2></div> 
            
            <div><h5>{{{truncate short_description 60}}}</h5></div>
            <hr>
            
            <div><h6>SKU: {{product_sku}}</h6></div> 
            <div><h5>MSRP: {{price}}</h5></div>
            
          </div> 
      </div> 

     <div class="row">
       <div class="col-12 mnContent">
          <div class="col-6">
            <div class="description">
            <p>{{{truncate descript1 920}}} {{{descriptBullets}}}</p>
            </div>
            </div>
          
              
               <div class="col-6">
               {{#each mnImage}}
                
                  <img src={{url_standard}} alt="the joy factory product image" width="445" height="445">
              
              {{/each}}

              <div class="col-12 image-box">

              {{#each images}}
                
                 <div class="col-4 pdf-thumb"><img src={{url_standard}} alt="the joy factory product image" width="112" height="112"></div> 
              
              {{/each}}
             </div> 
        </div>

        </div>
       
       
      </div>
      

      <div class="row">
